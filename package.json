{
  "name": "SleepDiary",
  "version": "1.0.0",
  "description": "Sleepdiary package.json",
  "author": "",
  "scripts": {
    "clean": "rimraf ./src/*.js ./src/*.js.map && npm cache clean",
    "clean-dist": "rimraf ./dist/*.js ./dist/*.map",
    "clean-dist-prod": "rimraf ./dist_prod/*.js ./dist_prod/*.map",
    "clean-install": "npm run clean && npm install",
    "start": "npm run dev",
    "dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js -server  --progress --display-error-details  --colors --port 3100 --inline --config webpack.config.js --content-base src",
    "test": "karma start",
    "build": "rimraf dist  && webpack -d --progress --profile --config webpack.config.js",
    "deploy": "webpack -p",
    "prod": "npm run clean & webpack --config webpack.production.config.js  --progress --profile --colors --display-error-details --optimize-minimize --optimize-dedupe --display-cached --content-base src",
    "watch": "webpack --watch --progress --profile",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "postinstall": "",
    "webdriver:update": "webdriver-manager update",
    "json-server": "json-server --watch db.json --routes routes.json --port 8081"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^4.0.3",
    "@angular/common": "4.0.3",
    "@angular/compiler": "4.0.3",
    "@angular/core": "4.0.3",
    "@angular/forms": "4.0.3",
    "@angular/http": "4.0.3",
    "@angular/platform-browser": "4.0.3",
    "@angular/platform-browser-dynamic": "4.0.3",
    "@angular/platform-server": "4.0.3",
    "@angular/router": "4.0.3",
    "@angular2-mdl-ext/popover": "^0.5.0",
    "@angular2-mdl-ext/select": "^0.8.1",
    "angular2-jwt": "^0.1.26",
    "angular2-mdl": "^2.8.0",
    "chart.js": "^2.4.0",
    "core-js": "2.4.1",
    "ie-shim": "0.1.0",
    "moment": "^2.17.1",
    "ng2-charts": "^1.4.4",
    "rxjs": "^5.0.2",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.54",
    "@types/q": "0.0.32",
    "@types/selenium-webdriver": "^2.53.36",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.4",
    "awesome-typescript-loader": "^3.0.0-beta.14",
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.0.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "^1.1.0",
