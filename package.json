{
  "name": "SleepDiary",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "clean": "rimraf ./src/*.js ./src/*.js.map && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "start": "webpack-dev-server  --progress --display-error-details  --colors --port 3100 --inline --config webpack.config.js --content-base src",
    "start.prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "clean-start": "npm run clean && npm start.dev",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist  && webpack -d --progress --profile --config webpack.config.js",
    "deploy": "webpack -p",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "json-server": "json-server --watch db.json --routes routes.json --port 8081"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/platform-server": "2.0.2",
    "@angular/router": "3.0.2",
    "angular2-jwt": "0.1.24",
    "angular2-mdl": "1.8.2",
    "core-js": "2.4.1",
    "ie-shim": "0.1.0",
    "moment": "2.15.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.4.1",
    "awesome-typescript-loader": "^2.2.4",
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.0.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "json-server": "^0.8.17",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "node-sass": "^3.9.0",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "tslint": "^3.7.4",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.3",
    "typings": "^1.0.4",
    "webpack": "^1.13.2",
    "webpack-dotenv-plugin": "^1.3.2",
    "webpack-merge": "^0.14.1"
  },
  "repository": {}
}
