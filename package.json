{
  "name": "sleep-diary",
  "version": "1.0.0",
  "description": "Sleepdiary package.json",
  "author": "",
  "scripts": {
    "clean": "rimraf ./src/*.js ./src/*.js.map && npm cache clean",
    "clean-dist": "rimraf ./dist/*.js ./dist/*.map",
    "clean-dist-prod": "rimraf ./dist_prod/*.js ./dist_prod/*.map",
    "clean-install": "npm run clean && npm install",
    "start": "echo \"NOOP! run '$ npm run dev' instead!!! \" && exit 0",
    "dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --inline --config webpack.config.js",
    "test": "karma start",
    "build": "rimraf dist  && webpack -d --progress --profile --config webpack.config.js",
    "deploy": "webpack -p",
    "prod": "npm run clean & webpack --define process.env.NODE_ENV=\"production\" --config webpack.production.config.js  --progress --profile --colors  --display-cached",
    "watch": "webpack --watch --progress --profile",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "postinstall": "",
    "webdriver:update": "webdriver-manager update",
    "json-server": "json-server --watch db.json --routes routes.json --port 8081"
  },
  "keywords": [],
  "license": "ISC",
